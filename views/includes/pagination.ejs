<section class="row justify-content-lg-center">
    <% if (currentPage !== 1 && previousPage !== 1) { %>
        <a class="border border-primary p-1 ml-3" href="?page=1">1</a>
    <% } %>
    <% if (hasPreviousPage) { %>
        <a class="border border-primary p-1 ml-3" href="?page=<%= previousPage %>"><%= previousPage %></a>
    <% } %>
    <a href="?page=<%= currentPage %>" class="border border-primary p-1 ml-3 active"><%= currentPage %></a>
    <% if (hasNextPage) { %>
        <a class="border border-primary p-1 ml-3" href="?page=<%= nextPage %>"><%= nextPage %></a>
    <% } %>
    <% if (lastPage !== currentPage && nextPage !== lastPage) { %>
        <a class="border border-primary p-1 ml-3" href="?page=<%= lastPage %>"><%= lastPage %></a>
    <% } %>
</section>